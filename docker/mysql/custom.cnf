[mysqld]

skip-host-cache
skip-name-resolve
explicit_defaults_for_timestamp=1

# charset for utf8mb4
character-set-server=utf8mb4
collation-server=utf8mb4_0900_ai_ci

# time zone
default-time-zone=UTC

max_connections=250

[mysqld_safe]

timezone=UTC

[client]

# charset for utf8mb4
default-character-set=utf8mb4
